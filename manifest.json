{
  "manifest_version": 2,

  "name": "Voice Browser",
  "description": "Allows Chrome users to google search via voice, and navigate back and forward in browser history",
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
    "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.0.min.js", "receiver.js"],
      "run_at": "document_end"
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}