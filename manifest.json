{
  "manifest_version": 2,

  "name": "Voice Browser",
  "description": "Allows Chrome users to google search via voice, and navigate back and forward in browser history",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides" : {
    "newtab": "home.html"
  },
  "browser_action": {
    "default_icon": "siri-icon.png"
  },
  "icons":{
    "16": "siri-icon-xs.png",
    "128": "siri-icon.png"
  }
}