{
  "manifest_version": 2,

  "name": "Nat Voice Commands",
  "short_name": "Nat",
  "description": "Voice commands for Chrome. Browse the web effortlessly using only your voice",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "N_48px.png"
  },
  "icons":{
    "16": "N_48px.png",
    "48": "icon_48x48_solid.png",
    "128": "icon_128x128.png"
  }
}