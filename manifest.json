{
  "manifest_version": 2,

  "name": "Nat Voice Commands",
  "short_name": "Nat",
  "description": "Voice commands for Chrome. Browse the web effortlessly using only your voice",
  "version": "1.0.2",
  "permissions": [
    "tabs",
    "storage",
    "identity",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "N_48px.png"
  },
  "oauth2": {
    "client_id": "558559751604-ci8he1nsgo81bgf6q3qda8o6513cbt5q.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "icons":{
    "16": "N_48px.png",
    "48": "icon_48x48_solid.png",
    "128": "icon_128x128.png"
  },
  "key": ""
}