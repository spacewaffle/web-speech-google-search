{
  "manifest_version": 2,

  "name": "Nat Voice Commands",
  "short_name": "Nat",
  "description": "Voice commands for Chrome. Browse the web effortlessly using only your voice",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "https://www.googleapis.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "N_48px.png"
  },
  "oauth2": {
    "client_id": "558559751604-r6knnadh89pckgqi3d2mip34h25cpnbe.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "icons":{
    "16": "N_48px.png",
    "48": "icon_48x48_solid.png",
    "128": "icon_128x128.png"
  }
}