{
  "manifest_version": 2,

  "name": "Nat",
  "description": "Allows Chrome users to google search via voice, and navigate back and forward in browser history",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  // "chrome_url_overrides" : {
  //   "newtab": "home.html"
  // },
  "browser_action": {
    "default_icon": "N_48px.png"
  },
  "icons":{
    "16": "N_48px.png",
    "48": "icon_48x48_solid.png",
    "128": "icon_128x128.png"
  }
}